set CONNECT_UNDER_RESET 1
set CORE_RESET 0
#elseif (($STLINK_DAP == 0) && $resetMode.name() == "HARD")
# use hardware reset
reset_config srst_only srst_nogate
set CONNECT_UNDER_RESET 0
set CORE_RESET 0
#elseif ($resetMode.name() == "SOFT" || $resetMode.name() == "NONE")
